{
  "name": "may",
  "version": "0.0.1",
  "description": "A discord bot that has something fresh in it",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheOnlyArtz/may.git"
  },
  "xo": {
    "space": 4,
    "semicolon": true,
    "rules": {
      "camelcase": 0,
      "new-parens": 0,
      "guard-for-in": 0,
      "prefer-const": 0,
      "no-eval": 0,
      "no-unused-vars": 0,
      "no-prototype-builtins": 0,
      "unicorn/filename-case": 0,
      "import/no-dynamic-require": 0
    }
  },
  "keywords": [
    "bot",
    "discord",
    "fast",
    "light",
    "makes",
    "you",
    "happy"
  ],
  "author": "TheOnlyArtz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheOnlyArtz/may/issues"
  },
  "homepage": "https://github.com/TheOnlyArtz/may#readme",
  "dependencies": {
    "artzlogger": "0.0.4",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "discord.js": "^11.1.0",
    "ffmpeg-binaries": "^3.2.2-3",
    "google": "^2.1.0",
    "google-translate-api": "^2.3.0",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "ms": "^2.0.0",
    "node-json-db": "^0.7.3",
    "node-opus": "^0.2.6",
    "opusscript": "0.0.3",
    "simple-youtube-api": "^3.0.0",
    "snekfetch": "^3.2.9",
    "spongeuscator": "^0.1.3",
    "xml2js": "^0.4.18",
    "youtube-info": "^1.1.1",
    "ytdl-core": "^0.15.2"
  }
}
